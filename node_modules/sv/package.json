{
  "name": "sv",
  "version": "0.6.5",
  "type": "module",
  "description": "A CLI for creating and updating SvelteKit projects",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sveltejs/cli",
    "directory": "packages/cli"
  },
  "homepage": "https://svelte.dev",
  "files": [
    "dist"
  ],
  "bin": "./dist/bin.js",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/lib/testing.d.ts",
      "default": "./dist/testing.js"
    }
  },
  "devDependencies": {
    "@types/degit": "^2.8.6",
    "@types/ps-tree": "^1.1.6",
    "@types/tar-fs": "^2.0.4",
    "commander": "^12.1.0",
    "degit": "^2.8.4",
    "empathic": "^1.0.0",
    "package-manager-detector": "^0.2.2",
    "picocolors": "^1.1.0",
    "ps-tree": "^1.2.0",
    "tar-fs": "^3.0.6",
    "tinyexec": "^0.3.1",
    "valibot": "^0.41.0",
    "@sveltejs/clack-prompts": "0.9.1",
    "@sveltejs/create": "0.0.0",
    "@sveltejs/addons": "0.0.0",
    "@sveltejs/cli-core": "0.0.0"
  },
  "keywords": [
    "create",
    "new",
    "project",
    "starter",
    "svelte",
    "sveltekit",
    "template",
    "wizard"
  ],
  "scripts": {
    "check": "tsc",
    "format": "pnpm lint --write",
    "lint": "prettier --check . --config ../../prettier.config.js --ignore-path ../../.gitignore --ignore-path .gitignore --ignore-path ../../.prettierignore"
  }
}